    <script>
        $(".frm_Keyword").on("submit", function (e) {
            e.preventDefault();
            $(".jsback").removeClass("hide");
            $(".jsSendMessageContainer").addClass("hide");
            $(".jsGethelpContainer").removeClass("hide");
            $(".jsMessageFormContainer").addClass("hide");
            $(".HelpKeywordContainer").removeClass("hide");
            $(".helpTitleMessage").addClass("hide");
            $(".helpTitleMessageSuccess").addClass("hide");
            $(".helpTitle").addClass("hide");
            $(".KeywordSearch1").removeClass("hide");
            $(".KeywordSearch2").addClass("hide");
            var Keyword = $(this).find("input").val();
            console.log($(this).serialize());
            $.ajax({
                url: GetBaseURL() + "Appendix/SearchTextdata",
                type: "POST",
                data: $(this).serialize(),
                dataType: "json",
                success: function (data) {
                    console.log(data);  //Keyword, Category, Definition
                    $(".HelpKeywordContainer").empty();
                    $(".HelpKeywordContainer").html("<h5><b>"+data.length+" Result(s) of "+Keyword+" found</b></h5>");
                    if (data.length > 0) {
                        $.each(data, function (key, value) {
                            var Definition = ' <div class="wa__popup_content_item"> <a class="wa__stt wa__stt_online" href="void:javascript(0)"> <div class="wa__popup_avatar"> <div class="wa__cs_img_wrap" style="background: url(https://w-a.live/app/pic/m_1-4a59e954d80f071ff6f558a0eb71b2e0.jpg) center center no-repeat; background-size: cover;"></div> </div> <div class="wa__popup_txt"> <div class="wa__member_duty"> <p style="margin-bottom:6px"> Keyword: <b style="color:#000">'+ value.Keyword +' </b> &nbsp;&nbsp;&nbsp; Category: <b style="color:#000">'+ value.Category +' </b> </p>' + value.Definition + ' </div> <div class="wa__member_status"> <font color="green"></font> </div> </div> </a> </div>';
                            $(".HelpKeywordContainer").append(Definition);
                        });
                    }
                    else
                        $(".HelpKeywordContainer").html("<h3> Sorry,<br/> the search keyword not  found </h3>");
                },
                error: function (data) {


                }
            });

         });

        $(".jsSendMessage").click(function () {
            $(".jsSendMessageContainer").addClass("hide");
            $(".jsGethelpContainer").addClass("hide");
            $(".jsMessageFormContainer").removeClass("hide");
            $(".jsback").removeClass("hide");
